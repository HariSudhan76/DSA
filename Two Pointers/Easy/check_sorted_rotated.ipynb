   "source": [
    "def check(nums) -> bool:\n",
    "    count = 0\n",
    "    if nums[0]<nums[-1]:\n",
    "        count+=1\n",
    "    for i in range(len(nums)-1):\n",
    "        if nums[i]>nums[i+1]:\n",
    "            count+=1\n",
    "        if count>1:\n",
    "            return False\n",
    "    return True\n",
    "nums = [3,4,5,1,2]\n",
    "check(nums)"
   ]
  }
