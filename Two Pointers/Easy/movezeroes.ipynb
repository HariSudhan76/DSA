    "def movezeroes(nums):\n",
    "    last = 0\n",
    "    for i in range(len(nums)):\n",
    "        if nums[i]!=0:\n",
    "            nums[i],nums[last] = nums[last],nums[i]\n",
    "            last +=1\n",
    "    return nums\n",
    "n=[0,1,0,3,12]\n",
    "movezeroes(n)"
   ]
  }
